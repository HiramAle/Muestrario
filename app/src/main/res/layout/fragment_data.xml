<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DataFragment">

    <TextView
        android:id="@+id/Item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="86dp"
        android:layout_marginStart="31dp"
        android:text="Name"
        android:fontFamily="@font/pangram_bold"
        android:gravity="center_horizontal|center_vertical"
        android:textSize="28sp"
        android:textColor="@color/text"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:orientation="vertical">
        <TextView
            android:paddingStart="31dp"
            android:layout_width="230dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pangram_medium"
            android:text="Clasificación del NRC"
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/nrc"
            android:paddingStart="35dp"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/angram_light"
            android:text="0"
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textAlignment="center"
            android:paddingTop="20dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pangram_medium"
                    android:text="MS"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/ms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/angram_light"
                    android:text="0"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pangram_medium"
                    android:text="PC"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/pc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/angram_light"
                    android:text="0"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pangram_medium"
                    android:text="EM"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/em"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/angram_light"
                    android:text="0"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
            </LinearLayout>

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pangram_medium"
                    android:text="Ca"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/ca"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/angram_light"
                    android:text="0"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pangram_medium"
                    android:text="P"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
                <TextView
                    android:id="@+id/p"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/angram_light"
                    android:text="0"
                    android:textAlignment="textStart"
                    android:textColor="@color/text"
                    android:textSize="20sp" />
            </LinearLayout>


        </LinearLayout>

        <TextView
            android:paddingStart="31dp"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pangram_medium"
            android:text="Característica especial"
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp"
            android:paddingTop="20dp"/>
        <TextView
            android:id="@+id/property"
            android:paddingStart="31dp"
            android:paddingEnd="35dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/angram_light"
            android:text="None"
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp" />

        <TextView
            android:paddingStart="31dp"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pangram_medium"
            android:text="Factores Antinutricionales "
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp"
            android:paddingTop="20dp"    />
        <TextView
            android:id="@+id/factors"
            android:paddingStart="31dp"
            android:paddingEnd="35dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/angram_light"
            android:text="None"
            android:textAlignment="textStart"
            android:textColor="@color/text"
            android:textSize="20sp" />


    </LinearLayout>

</FrameLayout>